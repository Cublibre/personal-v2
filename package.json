{
  "name": "cublibre",
  "version": "1.0.0",
  "private": true,
  "description": "cublibre",
  "author": "Christine Chen",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prettier": "prettier --config .prettierrc 'src/**/*.{js,jsx,ts,tsx}' 'content/**/*.md' --write",
    "eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
